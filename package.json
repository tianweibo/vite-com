    "@vue/test-utils": "2.0.2",
    "happy-dom": "6.0.4",
    "unocss": "0.45.6",
    "vite": "3.1.0",
    "vitepress": "1.0.0-alpha.4",
    "vitepress-theme-demoblock": "1.4.2",
    "vitest": "0.21.1",
    "web-norm": "^1.0.27",
    "eslint": "^7.25.0",
