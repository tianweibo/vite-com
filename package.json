    "inquirer": "^8.0.0"
  },
  "dependencies": {
    "vue": "3.2.37"
  },
  "lint-staged": {
    "*.{js,ts,vue,jsx,tsx}": [
      "npm run eslint"
    ],
    "*.{js,jsx,ts,tsx,md,html,css,lees,scss,sass}": "prettier --write"
